{
  "namedHosts": {
    "nci": {
      "url": "http://dapds00.nci.org.au/thredds",
      "software": "tds"
    },
    "csiro-thredds": {
      "software": "tds",
      "url": "http://rs-data1-mel.csiro.au/thredds"
    },
    "qld-geoserver": {
      "software": "geoserver",
      "url": "http://qld.auscover.org.au/geoserver/aus"
    }
  },
  "namedIntervals": {
    "modis": {
      "step": 8,
      "startMonth": 1,
      "startDay": 1
    }
  },
  "options": {
    "host": "nci",
    "transparent": true,
    "belowmincolor": "transparent",
    "abovemaxcolor": "transparent"
  },
  "themes": [
    {
      "name": "Biomass and Carbon",
      "icon": "fa-tree",
      "publications": [
        {
          "timestep": "annual",
          "options": {
            "timeFormat": "{{year}}-12-31"
          }
        }
      ],
      "layers": [
        {
          "name": "Vegetation Carbon Uptake",
          "description": "",
          "publications": [
            {
              "timestep": "daily",
              "options": {
                "filepath": "/ub8/au/OzWALD/daily/OzWALD.daily.GPP.{{year}}.nc",
                "colorscalerange": [
                  0,
                  10
                ],
                "timeFormat": "{{year}}-{{month}}-{{day}}",
                "variable": "GPP",
                "start": "2000-01-01",
                "end": "2016-12-31"
              }
            },
            {
              "timestep": "annual",
              "options": {
                "filepath": "/ub8/au/OzWALD/annual/OzWALD.annual.GPP.AnnualSums.nc",
                "colorscalerange": [
                  0,
                  4000
                ],
                "variable": "AnnualSums",
                "start": "2000-01-01",
                "end": "2016-12-31"
              }
            }
          ],
          "options": {
            "units": "gC/m^2",
            "palette": "BuGn",
            "numcolorbands": 9,
            "styles": "boxfill/anu_wald_darkgreen",
            "belowmincolor": "extend",
            "abovemaxcolor": "extend"
          }
        },
        {
          "name": "Tree cover",
          "description": "Percent of area classified as forest at 25 metre resolution mapping using Landsat imagery following the NCAS classification.",
          "options": {
            "filepath": "ub8/au/treecover/250m/ANUWALD.TreeCover.AllYears.250m.nc",
            "variable": "AllYears",
            "styles": "boxfill/anu_wald_darkgreen",
            "colorscalerange": [
              0,
              100
            ]
          }
        },
        {
          "name": "Site List",
          "description": "",
          "options": {
            "host": "qld-geoserver",
            "layer": "aus:biolib_sitelist_auscover"
          }
        },
        {
          "name": "Tree List",
          "description": "",
          "options": {
            "host": "qld-geoserver",
            "layer": "aus:biolib_treelist_auscover"
          }
        }
      ]
    },
    {
      "name": "Water",
      "icon": "fa-tint",
      "publications": [
        {
          "timestep": "daily",
          "options": {
            "filepath": "ub8/au/OzWALD/daily/OzWALD.daily.{{variable}}.{{year}}.nc",
            "timeFormat": "{{year}}-{{month}}-{{day}}"
          }
        },
        {
          "timestep": "annual",
          "options": {
            "filepath": "ub8/au/OzWALD/annual/OzWALD.annual.{{variable}}.AnnualMeans.nc",
            "layer": "AnnualMeans",
            "timeFormat": "{{year}}-12-31"
          }
        }
      ],
      "layers": [
        {
          "name": "Rainfall",
          "description": "Gridded rainfall",
          "options": {
            "units": "mm",
            "styles": "boxfill/rainbow",
            "abovemaxcolor": "extend",
            "numcolorbands": 9
          },
          "publications": [
            {
              "timestep": "daily",
              "options": {
                "palette": "Blues",
                "colorscalerange": [
                  0,
                  30
                ],
                "variable": "rain_day"
              }
            },
            {
              "timestep": "monthly",
              "options": {
                "palette": "Reds",
                "colorscalerange": [
                  0,
                  300
                ],
                "variable": "rain_month"
              }
            },
            {
              "timestep": "annual",
              "skip": true
            }
          ]
        },
        {
          "name": "Soil moisture",
          "description": "",
          "variable": "Ssoil",
          "options": {
            "palette": "anu_wald_blues09",
            "units": "mm",
            "mapOptions": {
              "styles": "boxfill/anu_wald_blues09"
            }
          }
        },
        {
          "name": "Runoff",
          "description": "",
          "variable": "Qtot",
          "options": {
            "palette": "anu_wald_blues09",
            "units": "mm",
            "mapOptions": {
              "styles": "boxfill/anu_wald_blues09"
            }
          }
        },
        {
          "name": "River Flows",
          "description": "",
          "variable": "Qriv",
          "options": {
            "palette": "anu_wald_blues09",
            "units": "mm",
            "mapOptions": {
              "styles": "boxfill/anu_wald_blues09"
            }
          }
        },
        {
          "name": "Inundation Extent",
          "description": "",
          "options": {
            "styles": "boxfill/anu_wald_bluescale20",
            "colorscalerange": [
              0,
              100
            ],
            "start": "2000-01-01",
            "end":"2016-12-31"
        },
          "publications": [
            {
              "timestep": "daily",
              "interval":"modis",
              "options":{
                "filepath": "ub8/au/owl/MOD09A1.OWLau.0_005deg.{{year}}.nc",
                "variable":"OWL",
                "layer":"OWL"
              }
            },
            {
              "timestep": "annual",
              "options": {
                "filepath": "ub8/au/owl/MOD09A1.OWLau.0_005deg.{{variable}}.nc",
                "variable": "AnnualMaxima",
                "layer": "AnnualMaxima"
              }
            }
          ]
        }
      ],
      "options": {
        "belowmincolor": "extend"
      }
    },
    {
      "name": "Biodiversity and Habitat",
      "icon": "fa-cloud",
      "options": {
        "host": "csiro-thredds"
      },
      "publications": [
        {
          "timestep": "daily",
          "options": {
            "filepath": "bawap/ncmls/{{variable}}/{{variable}}_{{decade}}.ncml",
            "timeFormat": "{{year}}-{{month}}-{{day}}",
            "start": "1900-01-01",
            "end": "yesterday"
