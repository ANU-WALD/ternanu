{
  "name": "Landscape Observatory",
  "namedHosts": {
    "nci": {
      "url": "http://dapds00.nci.org.au/thredds",
      "software": "tds"
    },
    "csiro-thredds": {
      "software": "tds",
      "url": "http://rs-data1-mel.csiro.au/thredds"
    },
    "qld-geoserver": {
      "software": "geoserver",
      "url": "http://qld.auscover.org.au/geoserver/aus"
    },
    "webhost": {
      "software": "static",
      "url": ""
    }
  },
  "namedIntervals": {
    "modis": {
      "step": 8,
      "startMonth": 1,
      "startDay": 1
    }
  },
  "options": {
    "host": "nci",
    "transparent": true,
    "belowmincolor": "transparent",
    "abovemaxcolor": "transparent",
    "styles": "boxfill/{{palette}}",
    "numcolorbands": 10
  },
  "themes": [
    {
      "name": "Biomass and Carbon",
      "icon": "fa-tree",
      "publications": [
        {
          "timestep": "annual",
          "options": {
            "timeFormat": "{{year}}-12-31"
          }
        }
      ],
      "layers": [
        {
          "name": "Tree cover",
          "description": "Percent of area classified as forest at 25 metre resolution mapping using Landsat imagery following the NCAS classification.",
          "options": {
            "filepath": "ub8/au/treecover/250m/ANUWALD.TreeCover.AllYears.250m.nc",
            "variable": "AllYears",
            "palette": "anu_wald_darkgreen",
            "colorscalerange": [
              0,
              100
            ]
          }
        },
        {
          "name": "Carbon Flux (OzFlux sites)",
          "options": {
            "host": "webhost",
            "filepath": "assets/static/ozflux.json",
            "vectors": "point"
          }
        },
        {
          "name": "Vegetation Carbon Uptake",
          "description": "",
          "publications": [
            {
              "timestep": "daily",
              "options": {
                "filepath": "/ub8/au/OzWALD/daily/OzWALD.daily.GPP.{{year}}.nc",
                "colorscalerange": [
                  0,
                  10
                ],
                "timeFormat": "{{year}}-{{month}}-{{day}}",
                "variable": "GPP",
                "start": "2000-01-01",
                "end": "2016-12-31"
              }
            },
            {
              "timestep": "annual",
              "options": {
                "filepath": "/ub8/au/OzWALD/annual/OzWALD.annual.GPP.AnnualSums.nc",
                "colorscalerange": [
                  0,
                  4000
                ],
                "variable": "AnnualSums",
                "start": "2000-01-01",
                "end": "2016-12-31"
              }
            }
          ],
          "options": {
            "units": "gC/m^2",
            "palette": "anu_wald_darkgreen",
            "numcolorbands": 9,
            "belowmincolor": "extend",
            "abovemaxcolor": "extend"
          }
        },
        {
          "name": "Fire Carbon Emissions",
          "options": {
            "palette": "anu_wald_gisheat_inv.v2"
          },
          "publications": [
            {
              "timestep": "daily",
              "options": {
                "filepath": "/ub8/au/FireEmissions/GFASv12.BurntCarbon.{{year}}.nc",
                "colorscalerange": [
                  0,
                  100
                ],
                "timeFormat": "{{year}}-{{month}}-{{day}}",
                "variable": "BurntCarbon",
                "start": "2003-01-01",
                "end": "2015-12-31"
              }
            },
            {
              "timestep": "annual",
              "options": {
                "filepath": "ub8/au/FireEmissions/GFASv12.BurntCarbon.AnnualSums.nc",
                "colorscalerange": [
                  0,
                  1500
                ],
                "variable": "AnnualSums",
                "start": "2000-01-01",
                "end": "2015-12-31"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "Biomass",
      "icon": "",
