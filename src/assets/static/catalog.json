      "layers": [
        {
          "name": "Rainfall",
          "description": "Gridded rainfall",
          "options": {
            "units": "mm",
            "styles": "boxfill/rainbow",
            "abovemaxcolor": "extend",
            "numcolorbands": 9,
            "host": "csiro-thredds"
          },
          "publications": [
            {
              "timestep": "daily",
              "options": {
                "filepath": "bawap/ncmls/{{variable}}/{{variable}}_{{decade}}.ncml",
                "timeFormat": "{{year}}-{{month}}-{{day}}",
                "start": "1900-01-01",
                "end": "yesterday",
                "palette": "Blues",
                "colorscalerange": [
                  0,
                  30
                ],
                "variable": "rain_day"
              }
            },
            {
              "timestep": "monthly",
              "options": {
                "filepath": "bawap/ncmls/{{variable}}.ncml",
                "timeFormat": "{{year}}-{{month}}-01",
                "palette": "Reds",
                "colorscalerange": [
                  0,
                  300
                ],
                "variable": "rain_month"
              }
            },
            {
              "timestep": "annual",
              "skip": true
            }
          ]
        },
        {
          "name": "Soil moisture (OzWALD)",
